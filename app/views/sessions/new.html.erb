<br><br>

<div class="row" style="padding-left: 10%;">
  <h5>Log-In</h5>
    <form class="col s6">
      <div class="row">
        <div class="input-field col s6">
          <i class="material-icons prefix">account_circle</i>
          <input id="icon_prefix" type="email" class="validate">
          <label class="active" for="icon_prefix">E-mail</label>
        </div>
        <div class="input-field col s6">
          <i class="material-icons prefix">key</i>
          <input id="icon_telephone" type="password" class="validate">
          <label class="active">Password</label>
        </div>
        <button class="btn waves-effect waves-light light-blue" type="submit" name="action">Log In
          <i class="material-icons right">key</i>
        </button>
      </div>
    </form>
  </div>

<button class="btn waves-effect waves-light white" style="margin-left: 10%;">
  <%= link_to('Log in with Google', '/auth/google_oauth2', method: :post) %>
</button>
